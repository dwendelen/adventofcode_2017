import Html exposing (..)
import Dict exposing (..)
import Array exposing (..)
import Debug exposing (..)

main = text <| toString <| solve input

type Dir = Up | Down | Left | Right

solve: String -> String
solve input =
    let
        maze = parse input
    in
        runMaze maze (findStart maze) Down ""

parse: String -> Array (Array Char)
parse input =
    String.split "\n" input
    |> List.map String.toList
    |> List.map Array.fromList
    |> Array.fromList
    
findStart: Array (Array Char) -> (Int, Int)
findStart maze =
    Array.get 0 maze
    |> Maybe.withDefault (Array.fromList [])
    |> Array.toIndexedList
    |> List.filter (\(i, v) -> v == '|')
    |> List.map Tuple.first
    |> List.head
    |> Maybe.map (\i -> (i, 0))
    |> Maybe.withDefault (0,0)

runMaze: Array (Array Char) -> (Int, Int) -> Dir -> String -> String
runMaze maze pos previousDirection acc =
    case getChar maze pos of
        ' ' -> acc
        '|' -> runMaze maze (applyDir pos previousDirection) previousDirection acc
        '-' -> runMaze maze (applyDir pos previousDirection) previousDirection acc
        '+' -> 
            let
                (newDir, newPos) = rotate maze pos previousDirection
            in
                runMaze maze newPos newDir acc 
        a -> runMaze maze (applyDir pos previousDirection) previousDirection (acc ++ (String.fromChar a))
        
getChar maze (x, y) =
    Array.get y maze
    |> Maybe.andThen (Array.get x)
    |> Maybe.withDefault ' '

applyDir (x, y) dir =
    case dir of
        Up -> (x, y-1)
        Down -> (x, y+1)
        Left -> (x-1, y)
        Right -> (x+1, y)

rotate: Array (Array Char) -> (Int, Int) -> Dir -> (Dir, (Int, Int))
rotate maze pos previousDirection =
    [Up, Down, Right, Left]
    |> List.filter (\d -> d /= (inverseDir previousDirection))
    |> List.map (\d -> (d, applyDir pos d))
    |> List.map (\(d, p) -> (d, p, getChar maze p))
    |> List.filter (\(d, p, c) -> c /= ' ')
    |> List.map (\(d, p, c) -> (d, p))
    |> List.head
    |> Maybe.withDefault (Up, (-1, -1))

inverseDir dir =
    case dir of
        Up -> Down
        Down -> Up
        Right -> Left
        Left -> Right
        
